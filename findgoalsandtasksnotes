content = self.driver.find_element_by_class_name('content')
goal_and_task_list = content.find_element_by_tag_name('ul')
for element in goal_and_task_list.find_elements_by_xpath('.//*'):
    html_id = element.get_attribute('id')
    if html_id:
        print(html_id)

